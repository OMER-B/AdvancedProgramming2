<UserControl x:Class="GUIProject.View.SettingsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GUIProject.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
      <Grid.ColumnDefinitions>
        <ColumnDefinition />
      </Grid.ColumnDefinitions>
      <Grid.RowDefinitions>
        <RowDefinition />
        <RowDefinition />
       
        <RowDefinition />
        <RowDefinition />
      </Grid.RowDefinitions>

        <TextBlock FontSize="60" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Grid.Row="0">Settings</TextBlock>
        <ListBox Grid.Row="1" ItemsSource="{StaticResource ViewModel.SettingsVM.test}"></ListBox>
        <Button Content="Remove" Grid.Row="2"></Button>
        <TextBlock FontSize="20" FontWeight="Bold" Grid.Row="3">2005 Products Shipped</TextBlock>

    </Grid>
</UserControl>
