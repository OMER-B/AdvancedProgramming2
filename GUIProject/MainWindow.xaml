        xmlns:local="clr-namespace:GUIProject"
        xmlns:views="clr-namespace:GUIProject.View"
        mc:Ignorable="d"
        Title="ex2" Height="450" Width="800">
    <Grid>
        <TabControl HorizontalAlignment="Left"  VerticalAlignment="Top" Height="420" Width="794">
            <TabItem Header="Settings">
