@model ImageServiceWeb.Models.LogModel

@{
    ViewBag.Title = "Logs";
}

<h2>Logs</h2>


<table>
    @{ foreach (var v in Model.List)
        {
            <tr>
                <td>
                    @v.Title
                </td>
                <td>
                    @v.Content
                </td>
            </tr>
        }
    }
</table>